{
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.colorTheme": "Evondev Dracula Night Contrast",
  "workbench.iconTheme": "material-icon-theme",
  "powermode.enabled": true,
  "powermode.combo.location": "off",
  "powermode.combo.counterEnabled": "hide",
  "powermode.shake.enabled": false,
  "powermode.explosions.size": 5,
  "editor.fontFamily": "SF Mono, 'Courier New', monospace,Dank Mono",
  "editor.fontLigatures": true,
  "terminal.integrated.localEchoStyle": "dim",
  // "indentRainbow.colors": [
  //   "rgba(24, 78, 119,0.15)",
  //   "rgba(30, 96, 145,0.15)",
  //   "rgba(26, 117, 159, 0.15)",
  //   "rgba(22, 138, 173,0.15)",
  //   "rgba(52, 160, 164,0.15)",
  //   "rgba(82, 182, 154,0.15)",
  //   "rgba(82, 182, 154,0.1)",
  //   "rgba(82, 182, 154,0.05)",
  //   "rgba(82, 182, 154,0.025)"
  // ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.autoSave": "afterDelay",
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 1,
  "vscode_custom_css.imports": ["file:///D://VSCode/VSCodeCustom.css"],
  "markdown.preview.fontSize": 15,
  "emmet.triggerExpansionOnTab": true
}
